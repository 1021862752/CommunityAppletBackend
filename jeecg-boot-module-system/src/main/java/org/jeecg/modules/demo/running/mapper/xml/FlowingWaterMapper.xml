<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.demo.running.mapper.FlowingWaterMapper">

    <!-- 根据时间域获取流水信息 -->
    <select id="getWaterByDate" resultType="org.jeecg.modules.demo.running.entity.FlowingWater">
        select * from flowing_water where date_str=#{dateStr}
    </select>

</mapper>